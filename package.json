{
    "name": "ardmap",
    "description": "Agrostream project",
    "version": "1.4.2",
    "author": "Agrostream (http://agroplan.azurewebsites.net)",
    "private": true,
    "scripts": {
        "start": "webpack-dev-server --progress --config webpack.dev.js",
        "dev": "npm start",
        "eslint": "eslint --ext .vue --ext .js src/",
        "eslint:fix": "eslint --fix --ext .vue --ext .js src/",
        "eslint:root": "eslint --ignore-pattern 'src/**/*' --ext .js .",
        "eslint:root:fix": "eslint --fix --ignore-pattern 'src/**/*' --ext .js .",
        "puglint": "pug-lint-vue src",
        "build": "webpack --progress --hide-modules --config webpack.prod.js",
        "build:test": "webpack --progress --hide-modules --config webpack.test.js",
        "build:copy": "cp -R dist/* serve_dist/",
        "build:test:copy": "npm run build:test && npm run build:copy",
        "serve:dist": "http-server ./serve_dist -p 8082 -a localhost --gzip",
        "build:test:patch": "webpack --progress --hide-modules --env.patch --config webpack.test.js",
        "build:test:minor": "webpack --progress --hide-modules --env.minor --config webpack.test.js",
        "build:test:major": "webpack --progress --hide-modules --env.major --config webpack.test.js"
    },
